   "source": [
    "def isPalindrome(res):\n",
    "    return res == res[::-1]\n",
    "\n",
    "res = input('Enter the string you want to check: ')\n",
    "ans = isPalindrome(res)\n",
    "\n",
    "if ans:\n",
    "    print(\"{} is a Palindrome String\".format(res))\n",
    "else:\n",
    "    print(\"{} is not a Palindrome String\".format(res))"
   ]
